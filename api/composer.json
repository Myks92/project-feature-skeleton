{
  "name": "myks92/project-feature-skeleton",
  "type": "project",
  "license": "MIT",
  "description": "Sport Federation",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.13",
    "knplabs/knp-paginator-bundle": "^5.8",
    "ramsey/uuid": "^4.4",
    "symfony/console": "6.2.*",
    "symfony/dotenv": "6.2.*",
    "symfony/flex": "^2.2",
    "symfony/framework-bundle": "6.2.*",
    "symfony/messenger": "6.2.*",
    "symfony/runtime": "6.2.*",
    "symfony/security-bundle": "6.2.*",
    "symfony/serializer": "6.2.*",
    "symfony/translation": "6.2.*",
    "symfony/twig-bundle": "6.2.*",
    "symfony/validator": "6.2.*",
    "symfony/yaml": "6.2.*",
    "twig/twig": "^3.4",
    "webmozart/assert": "^1.11"
  },
  "require-dev": {
    "dg/bypass-finals": "^1.4",
    "friendsofphp/php-cs-fixer": "^3.16",
    "icanhazstring/composer-unused": "^0.8.4",
    "overtrue/phplint": "^5.3",
    "phpunit/phpunit": "10",
    "psalm/plugin-phpunit": "^0.18.4",
    "psalm/plugin-symfony": "^5.0",
    "qossmic/deptrac-shim": "^1.0",
    "rector/rector": "^0.15.25",
    "symfony/browser-kit": "^6.1",
    "symfony/debug-bundle": "6.2.*",
    "symfony/phpunit-bridge": "^6.1",
    "symfony/stopwatch": "6.2.*",
    "symfony/var-dumper": "6.2.*",
    "symfony/web-profiler-bundle": "6.2.*",
    "vimeo/psalm": "^5.9"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "lint": "phplint",
    "php-cs-fixer": "php-cs-fixer --config=.php-cs-fixer.php --allow-risky=yes",
    "psalm": "psalm --config=psalm.xml",
    "deptrac": "deptrac --config-file=deptrac.yaml --cache-file=var/cache/.deptrac.cache",
    "rector": "rector",
    "unused": "composer-unused",
    "test": "phpunit --colors=always",
    "test-coverage": "XDEBUG_MODE=coverage phpunit --colors=always --coverage-html var/test/coverage",
    "app": "php bin/console",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.2.*"
    }
  }
}
